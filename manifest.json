{
  "manifest_version": 2,
  "homepage_url": "https://github.com/dodying/newtab-pinned",
  "name": "__MSG_extName__",
  "version": "0.0.2",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "commands": {
    "close-tab": {
      "suggested_key": {
        "default": "Alt+W"
      }
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+P"
      }
    }
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
